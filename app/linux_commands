select * from business_pattern where name like 'Carolina Cu%' and company = 1

select image, imagethumb from business_pattern where name like 'Carolina Cu%' and company = 1

update business_pattern set image = 'cliente.png' imagethumb = 'cliente.png' where name like 'Carolina Cu%' and company = 1

update business_pattern set image = 'cliente.png', imagethumb = 'cliente.png' where name like 'Carolina Cu%' and company = 1

select company, name, image from business_pattern where image = '' or image = 'thumb_F973675429076YBBTOR.jpg.png';  
update business_pattern set image = 'cliente.png' where image = '' or image = 'thumb_F973675429076YBBTOR.jpg.png';  

select company, name, imagethumb from business_pattern where imagethumb = '' or imagethumb = 'thumb_F973675429076YBBTOR.jpg.png';  
update business_pattern set imagethumb = 'cliente.png' where imagethumb = '' or imagethumb = 'thumb_F973675429076YBBTOR.jpg.png';  

