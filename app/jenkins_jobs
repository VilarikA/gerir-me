rm --force /tmp/killall.so  > /dev/null
rm --force /tmp/gates.lod  > /dev/null
rm --force /tmp/yam > /dev/null
rm --force /tmp/ssy > /dev/null
rm --force /tmp/yumcc > /dev/null
