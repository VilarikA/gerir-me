select bp.id, idforcompany as codigoexterno, bp.name as nome, bp.short_name as nomereduzido,
bp.sex as sexo, bp.birthday as nascimento, bp.document as cpf,
bp.phone as fone1, bp.mobile_phone as fone2, 
bp.email_alternative as fone3, bp.email, bp.createdat as criadoem, cs.name as estadocivil, ug.name as grupo,
cu.name as unidade,
bp.street as rua, bp.number_c as numero, bp.complement as complemento, bp.district as bairro, bp.postal_code as cep,
ci.name as cidade, st.short_name as uf,
bp.obs, is_employee colaborador, valueinaccount as saldo, bp_indicatedby as indicadopor
from business_pattern bp
left join civilstatus cs on cs.id = civilstatus
left join usergroup ug on ug.id = group_c
left join companyunit cu on cu.id = bp.unit
left join city ci on ci.id = cityref
left join state st on st.id = stateref
where bp.company = 35 order by bp.name, is_employee, bp.createdat
limit 50000


